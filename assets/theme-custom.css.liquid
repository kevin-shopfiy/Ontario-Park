@font-face {
  font-family: "Calibri";
  src: url({{ Calibri-Bold-Italic | asset_url }}) format("woff"),
  src: url({{ Calibri-Bold | asset_url }}) format("woff"),
  src: url({{ Calibri-Italic | asset_url }}) format("woff"),
  src: url({{ Calibri-Light | asset_url }}) format("woff"),
  src: url({{ Calibri-Regular | asset_url }}) format("woff");
}

h1, h2, h3 {
	font-family: "Calibri"!important; 
}


